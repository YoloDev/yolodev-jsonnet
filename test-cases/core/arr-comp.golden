local
    $arr = [
      1,
      2,
      3,
    ];
std["join"](
  [
  ],
  std["makeArray"](
    std["length"](
      $arr,
    ),
      function (
      $i = error "Parameter not bound"
    )
        local
          x = $arr[$i];

        if true
        then local
            $arr = [
              4,
              5,
              6,
            ];
        std["join"](
          [
          ],
          std["makeArray"](
            std["length"](
              $arr,
            ),
              function (
              $i = error "Parameter not bound"
            )
                local
                  y = $arr[$i];
              [
                x
                  * y,
              ],
          ),
        )
        else [
        ],
  ),
)
